{{/* Base header: common <head> section, Bootstrap includes, and theme
   initialization. Included at the top of every page. */}}
<!DOCTYPE html>
<html lang="en" data-bs-theme="auto">
    <head>
        <!-- Meta -->
        <meta charset="UTF-8">
        <title>Git@CoenDevelop - Access</title>
        <meta name="viewport" content="width=device-width, initial-scale=1.0">


        <!--Boostrap/Popper -->
        <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.8/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-sRIl4kxILFvY47J16cr9ZwB07vP4J8+LH7qKQnuqkuIAvNWLzeN8tE5YBujZqJLB" crossorigin="anonymous">
        <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.8/dist/js/bootstrap.bundle.min.js" integrity="sha384-FKyoEForCGlyvwx9Hj09JcYn3nv7wiPVlz7YYwJrWVcXK/BmnVDxM+D2scQbITxI" crossorigin="anonymous"></script>
    
        <style>
            /* Fix Prism colors for Dark Mode */
            [data-bs-theme='dark'] pre[class*="language-"],
            [data-bs-theme='dark'] code[class*="language-"] {
                background: var(--bs-body-tertiary-bg) !important;
                text-shadow: none !important;
            }

            .line-numbers-rows {
                border-right: 1px solid var(--bs-border-color) !important;
            }

        </style>

        <script>
            // Ensure theme is initialized even on pages that don't include the navbar (auth.tmpl)
            (function(){
                try {
                    const setTheme = theme => {
                        if (theme === 'auto' && window.matchMedia('(prefers-color-scheme: dark)').matches) {
                            document.documentElement.setAttribute('data-bs-theme', 'dark')
                        } else {
                            document.documentElement.setAttribute('data-bs-theme', theme)
                        }
                    }
                    const storedTheme = localStorage.getItem('theme') || 'auto'
                    setTheme(storedTheme)
                } catch (e) { /* ignore in old browsers */ }
            })();
        </script>
    </head>
